<rdf:RDF 
         xmlns:gleif-base="https://www.gleif.org/ontology/Base/"
         xmlns:gleif-ra="https://www.gleif.org/ontology/RA/"
         xmlns:gleif-L1="https://www.gleif.org/ontology/L1/"
         xmlns:gleif-RA-data="https://www.gleif.org/sample/RAData/"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:lcc-3166-1-add="http://www.omg.org/spec/LCC/Countries/ISO3166-1-CountryCodes-Add/"
