	.

gleif-L1:InternationalOrganization
	a owl:Class ;
	rdfs:subClassOf gleif-L1:LegalEntity ;
	rdfs:label "international organization" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:altLabel
		"intergovernmental organization" 
		;
	skos:definition "The legal entity is a non-resident unit created by international agreements or other arrangements for the provision of non-market services or financial intermediation at the international level." ;
	skos:prefLabel "international organization" ;
	.

gleif-L1:LEI
	a
		owl:DatatypeProperty ,
		owl:FunctionalProperty
		;
	rdfs:subPropertyOf gleif-base:hasTag ;
	rdfs:label "LEI" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	rdfs:range xsd:string ;
	skos:altLabel "has LEI string" ;
	skos:definition "The ISO 17442 compatible identifier for the legal entity recorded." ;
	skos:prefLabel "LEI" ;
	.

gleif-L1:LegalEntity
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:LegalPerson ,
		gleif-L1:RegisteredEntity
		;
	rdfs:label "legal entity" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "LEI-registered entities that are legally or financially responsible for the performance of financial transactions or have the legal right in their jurisdiction to enter independently into legal contracts, regardless of whether they are incorporated or constituted in some other way (e.g. trust, partnership, contractual). It excludes natural persons, but includes governmental organizations and supranationals." ;
	.

gleif-L1:LegalEntityEvent
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:Event ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasEventKind ;
			owl:onClass [
				a owl:Class ;
				owl:oneOf (
					gleif-L1:LegalEntityEventKindChangeLegalName
					gleif-L1:LegalEntityEventKindChangeOtherNames
					gleif-L1:LegalEntityEventKindChangeLegalAddress
					gleif-L1:LegalEntityEventKindChangeHQAddress
					gleif-L1:LegalEntityEventKindChangeLegalForm
					gleif-L1:LegalEntityEventKindDemerger
					gleif-L1:LegalEntityEventKindSpinoff
					gleif-L1:LegalEntityEventKindAbsorption
					gleif-L1:LegalEntityEventKindAcquisitionBranch
					gleif-L1:LegalEntityEventKindTransformationBranchToSubsidiary
					gleif-L1:LegalEntityEventKindTransformationSubsidiaryToBranch
					gleif-L1:LegalEntityEventKindTransformationUmbrellaToStandalone
					gleif-L1:LegalEntityEventKindBreakup
					gleif-L1:LegalEntityEventKindMergersAndAcquisitions
					gleif-L1:LegalEntityEventKindBankruptcy
					gleif-L1:LegalEntityEventKindLiquidation
					gleif-L1:LegalEntityEventKindVoluntaryArrangement
					gleif-L1:LegalEntityEventKindInsolvency
					gleif-L1:LegalEntityEventKindDissolution
				) ;
			] ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasEventStatus ;
			owl:onClass [
				a owl:Class ;
				owl:oneOf (
					gleif-L1:LegalEntityEventStatusInProgress
					gleif-L1:LegalEntityEventStatusWithdrawnCancelled
					gleif-L1:LegalEntityEventStatusCompleted
				) ;
			] ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasEffectiveDate ;
			owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:dateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasRecordedDate ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:dateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasValidationDocuments ;
			owl:onClass gleif-base:ValidationDocumentsKind ;
			owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasValidationReference ;
			owl:onDataRange xsd:string ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
	;
	rdfs:label "legal entity event" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "A single event specified by the LEI ROC in the life of a legal entity that would impact the Reference Data in the Global LEI System."
.

gleif-L1:LegalEntityEventGroup
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasGroupedEvent ;
			owl:someValuesFrom gleif-L1:LegalEntityEvent ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasLegalEntityEventGroupKind ;
			owl:onClass [
				a owl:Class ;
				owl:oneOf (
					gleif-L1:LegalEntityEventGroupKindReverseTakeover
					gleif-L1:LegalEntityEventGroupKindChangeLegalFormAndName
					gleif-L1:LegalEntityEventGroupKindComplexChangeLegalForm
				) ;
			] ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ;
	rdfs:label "legal entity event group" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "A complex event." ;
.

gleif-L1:LegalEntitySequencedEvent 
	a owl:Class ;
	rdfs:subClassOf 
			[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasGroupedEvent ;
			owl:onClass gleif-L1:LegalEntityEvent ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
			[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasSequenceNumber ;
			owl:onDataRange xsd:positiveInteger ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] 
 	;
	rdfs:label "legal entity sequenced event" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "An event in a group where the sequence is significant."
.

gleif-L1:LegalEntityEventKind
