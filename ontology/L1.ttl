<https://www.gleif.org/ontology/L1/>
	a
		voaf:Vocabulary ,
		owl:Ontology
		;
	rdfs:label "GLEIF L1 Ontology" ;
	dct:abstract """Ontology for legal entity identifier registration. It was designed for Global Legal Entity Identifier Foundation (GLEIF) Level 1 data corresponding to the Common Data Format version 2.1. It covers key reference data for a legal entity identifiable with an LEI. The ISO 17442 standard developed by the International Organization for Standardization defines a set of attributes or LEI reference data that comprises the most essential elements of identification. It specifies the minimum reference data, which must be supplied for each LEI:
    The official name of the legal entity as recorded in the official registers.
    The registered address of that legal entity.
    The country of formation.
    The codes for the representation of names of countries and their subdivisions.
    The date of the first LEI assignment; the date of last update of the LEI information; and the date of expiry, if applicable."""@en ;
	dct:contributor
		<http://purl.org/bryon> ,
		<http://www.workingontologist.com> ,
		<https://www.linkedin.com/in/peterivett/>
		;
	dct:creator gleif-base:GLEIF ;
	dct:issued "2022-01-06T00:00:00Z"^^xsd:dateTime ;
	dct:license "https://www.gleif.org/en/meta/lei-data-terms-of-use/"^^xsd:anyURI ;
	dct:rights "Copyright (c) 2019-2022 Global Legal Entity Identifier Foundation (GLEIF)" ;
	dct:rights "Copyright (c) 2021-2022 agnos.ai UK Ltd." ;
	dct:rightsHolder gleif-base:GLEIF ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-3-1"^^xsd:anyURI ;
	dct:title "Global Legal Entity Identifier Foundation Level 1 Ontology - Who Is Who"@en ;
	vann:preferredNamespacePrefix "gleif-L1" ;
	vann:preferredNamespaceUri "https://www.gleif.org/ontology/L1/" ;
	owl:imports
		<https://www.gleif.org/ontology/Base/> ,
		<https://www.gleif.org/ontology/EntityLegalForm/> ,
		<https://www.gleif.org/ontology/RegistrationAuthority/>
		;
	owl:versionIRI <https://www.gleif.org/ontology/L1-v1.1/L1/> ;
	skos:changeNote "Version 1.0.1 made a minor fix to 2 restrictions on LegalEntityRelationship"@en ;
	skos:changeNote "Version 1.1 reflects GLEIF LEI-CDF version 3.1, and removes wrong superclass of ValidationSourceKind"@en ;
	.

gleif-L1:Branch
	a owl:Class ;
	rdfs:subClassOf gleif-L1:RegisteredEntity ;
	rdfs:label "branch" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The entity is a branch of another legal entity." ;
	.

gleif-L1:BusinessRegistryIdentifier
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:RegistryIdentifier ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasOtherAuthority ;
			owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasRegisteredAuthority ;
			owl:onClass gleif-ra:BusinessRegistry ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:identifies ;
			owl:onClass gleif-L1:RegisteredEntity ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-L1:hasEntityID ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			owl:intersectionOf (
				[
					a owl:Class ;
					owl:unionOf (
						[
							a owl:Restriction ;
							owl:onProperty gleif-L1:hasRegisteredAuthority ;
							owl:onClass gleif-ra:BusinessRegistry ;
							owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
						]
						[
							a owl:Restriction ;
							owl:onProperty gleif-L1:hasOtherAuthority ;
							owl:cardinality "1"^^xsd:nonNegativeInteger ;
						]
					) ;
				]
				[
					a owl:Class ;
					owl:complementOf [
						a owl:Class ;
						owl:intersectionOf (
							[
								a owl:Restriction ;
								owl:onProperty gleif-L1:hasRegisteredAuthority ;
								owl:someValuesFrom gleif-ra:BusinessRegistry ;
							]
							[
								a owl:Restriction ;
								owl:onProperty gleif-L1:hasOtherAuthority ;
								owl:minCardinality "1"^^xsd:nonNegativeInteger ;
							]
						) ;
					] ;
				]
			) ;
		]
		;
	rdfs:label "business registry identifier" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The legal entity is a branch of another legal entity." ;
	.

gleif-L1:CentralGovernment
	a owl:Class ;
	rdfs:subClassOf gleif-L1:ResidentGovernmentEntity ;
	rdfs:label "central government" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:altLabel
		"central government entity" 
		;
	skos:definition "The legal entity is in a central government subsector which consists of the institutional units plus non-profit institutions (NPI) controlled by the central government institutions." ;
	skos:prefLabel "central government" ;
	.

gleif-L1:Fund
	a owl:Class ;
	rdfs:subClassOf gleif-L1:RegisteredEntity ;
	rdfs:label "fund" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The legal entity is an investment fund defined as a collective investment scheme (or pooled investment) beneficially owned by multiple investors and managed on behalf of those investors by an asset manager or by the fund itself." ;
	.

gleif-L1:FundFamily
