	gleif-base:hasTag "CHANGE_HQ_ADDRESS" ;
.

gleif-L1:LegalEntityEventKindChangeLegalForm
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "change legal form" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "Change in the legal form of the legal entity." ;
	gleif-base:hasTag "CHANGE_LEGAL_FORM" ;
.

gleif-L1:LegalEntityEventKindDemerger
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "demerger" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "Distribution of securities issued by another legal entity. The distributed securities may either be of a newly created or of an existing legal entity. For example, spin-off, demerger, unbundling, divestment." ;
	gleif-base:hasTag "DEMERGER" ;
.

gleif-L1:LegalEntityEventKindSpinoff
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "spinoff" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "A special case of demerger where the shareholders of the original entity are compensated for the value loss of the original entity via shares of the new entity or via dividend." ;
	gleif-base:hasTag "SPINOFF" ;
.

gleif-L1:LegalEntityEventKindAbsorption
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "absorption" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "Absorption is a form of merger where there is a combination of two or more companies into an 'existing company'. In the case of absorption, only one company survives and all others lose their identity." ;
	gleif-base:hasTag "ABSORPTION" ;
.

gleif-L1:LegalEntityEventKindAcquisitionBranch
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "acquisition branch" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The acquiring legal entity purchases an international branch entity." ;
	gleif-base:hasTag "ACQUISITION_BRANCH" ;
.

gleif-L1:LegalEntityEventKindTransformationBranchToSubsidiary
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "transformation branch to subsidiary" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The transfer of all of the assets and liabilities of an International Branch to the new subsidiary entity in exchange for the transfer of securities representing the capital of the subsidiary entity receiving the transfer." ;
	gleif-base:hasTag "TRANSFORMATION_BRANCH_TO_SUBSIDIARY" ;
.

gleif-L1:LegalEntityEventKindTransformationSubsidiaryToBranch
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "transformation subsidiary to branch" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The transfer of all of the assets and liabilities of a subsidiary to an International Branch entity in exchange for the transfer of securities representing the capital of the International Branch entity receiving the transfer." ;
	gleif-base:hasTag "TRANSFORMATION_SUBSIDIARY_TO_BRANCH" ;
.

gleif-L1:LegalEntityEventKindTransformationUmbrellaToStandalone
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "transformation umbrella to standalone" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "Change in legal form from a Fund legal entity structure with one or more than one sub-funds/compartments to a Fund legal entity structure without sub-funds/compartments." ;
	gleif-base:hasTag "TRANSFORMATION_UMBRELLA_TO_STANDALONE" ;
.

gleif-L1:LegalEntityEventKindBreakup
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "break-up" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "A break-up is a corporate action in which a single company splits into two or more independent, separately-run companies.  Regulators also can mandate break-ups of companies for anti-trust reasons." ;
	gleif-base:hasTag "BREAKUP" ;
.

gleif-L1:LegalEntityEventKindMergersAndAcquisitions
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "mergers and acquisitions" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The consolidation of Legal Entities or assets through various types of financial transactions, including mergers, acquisitions, consolidations, tender offers, purchase of assets and management acquisitions." ;
	gleif-base:hasTag "MERGERS_AND_ACQUISITIONS" ;
.

gleif-L1:LegalEntityEventKindBankruptcy
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "bankruptcy" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "Legal status of a legal entity unable to pay creditors. Bankruptcy usually involves a formal court ruling. Securities may become valueless (event completed)." ;
	gleif-base:hasTag "BANKRUPTCY" ;
.

gleif-L1:LegalEntityEventKindLiquidation
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "liquidation" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "Distribution of cash, assets or both of a legal entity. Debt may be paid in order of priority based on preferred claims to assets specified by the security (event completed)." ;
	gleif-base:hasTag "LIQUIDATION" ;
.

gleif-L1:LegalEntityEventKindVoluntaryArrangement
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "voluntary arrangement" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "A procedure that allows a legal entity to settle debts by paying only a proportion of the amount that it owes to creditors or to come to some other arrangement with its creditors over the payment of its debts (event completed)." ;
	gleif-base:hasTag "VOLUNTARY_ARRANGEMENT" ;
.

gleif-L1:LegalEntityEventKindInsolvency
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "insolvency" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "The entry of a decree or order by a court or agency or supervisory authority having jurisdiction in the premises the appointment of a trustee-in-bankruptcy or similar official for such party in any insolvency, readjustment of debt, marshalling of assets and liabilities, or similar proceedings, or for the winding up or liquidation of their respective affairs (event completed)." ;
	gleif-base:hasTag "INSOLVENCY" ;
.

gleif-L1:LegalEntityEventKindDissolution
	a
		owl:NamedIndividual ,
		gleif-L1:LegalEntityEventKind ;
	rdfs:label "dissolution" ;
	rdfs:isDefinedBy <https://www.gleif.org/ontology/L1/> ;
	skos:definition "(i) A voluntary termination of operations, (ii) a general assignment for the benefit of the legal entityâ€™s creditors or (iii) any other liquidation, dissolution or winding up of the legal entity (excluding a Liquidity Event), whether voluntary or involuntary (event completed)." ;
	gleif-base:hasTag "DISSOLUTION" ;
.

