# baseURI: https://www.gleif.org/ontology/Base/
# imports: http://www.omg.org/spec/LCC/Countries/CountryRepresentation/

@base <https://www.gleif.org/ontology/Base/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix gleif-base: <https://www.gleif.org/ontology/Base/> .
@prefix lcc-cr: <http://www.omg.org/spec/LCC/Countries/CountryRepresentation/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://www.gleif.org/ontology/Base/>
	a owl:Ontology ;
	rdfs:label "GLEIF Base Ontology" ;
	dct:abstract "This ontology defines generic concepts for reuse by other GLEIF ontologies" ;
	dct:license "http://opensource.org/licenses/MIT"^^xsd:anyURI ;
	dct:rights "Copyright (c) 2018 GLEIF" ;
	owl:imports
    <http://www.omg.org/spec/LCC/Countries/CountryRepresentation/>
		;
	.

gleif-base:ActiveStatus
	a
		owl:NamedIndividual ,
		gleif-base:EntityStatus
		;
	rdfs:label "active status" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "as of the last report or update, the entity was legally registered and operating" ;
	gleif-base:hasTag "ACTIVE" ;
	.

gleif-base:EntityExpirationReason
	a owl:Class ;
	rdfs:label "entity expiration reason" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "the reason that a legal entity ceased to exist and/or operate" ;
	.

gleif-base:EntityExpirationReasonDissolved
	a gleif-base:EntityExpirationReason ;
	rdfs:label "DISSOLVED" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "the entity ceased to operate" ;
	.

gleif-base:EntityStatus
	a owl:Class ;
	rdfs:label "entity status" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "a lifecycle stage indicating the operational and/or legal status of an entity as specified in a given business registry for that entity" ;
	skos:note "In some jurisdictions, there may be other possible values for entity status, such as suspended in the State of California, thus the individuals provided herein are not intended to be exhaustive." ;
	.

gleif-base:ExpirationReasonCorporateAction
	a gleif-base:EntityExpirationReason ;
	rdfs:label "CORPORATE_ACTION" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "the entity was acquired or merged with another entity" ;
	.

gleif-base:ExpirationReasonOther
	a gleif-base:EntityExpirationReason ;
	rdfs:label "OTHER" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "The reason for expiry is neither of DISSOLVED nor CORPORATE_ACTION" ;
	.

gleif-base:FormalOrganization
	a owl:Class ;
	rdfs:subClassOf gleif-base:Organization ;
	rdfs:label "formal organization" ;
	dct:source "W3C Organization Ontology" ;
	skos:definition "An Organization that is recognized in some legal jurisdiction, with associated rights and responsibilities. Examples include a Corporation, Charity, Government or Church." ;
	.

gleif-base:GLEIF
	a gleif-base:RegistrationAuthority ;
	gleif-base:hasLegalName "Global Legal Entity Identifier Federation" ;
	gleif-base:hasLocalLegalName "Global Legal Entity Identifier Federation" ;
	.

gleif-base:IdentificationScheme
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:Scheme ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:scopes ;
			owl:someValuesFrom gleif-base:Identifier ;
		]
		;
	rdfs:label "identification scheme" ;
	dct:source "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
	skos:definition "system for allocating identifiers to objects" ;
	.

gleif-base:Identifier
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:identifies ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasScope ;
			owl:onClass gleif-base:IdentificationScheme ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasTag ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "identifier" ;
	dct:source "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
	skos:definition "sequence of characters, capable of uniquely identifying that with which it is associated, within a specified context" ;
	.

gleif-base:InactiveStatus
	a
		owl:NamedIndividual ,
		gleif-base:EntityStatus
		;
	rdfs:label "inactive status" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "the entity that was assigned the identifier (business, LEI, or other) is no longer legally registered and/or operating, whether as a result of business closure, acquisition by or merger with another (or new) entity, or determination of illegitimacy" ;
	gleif-base:hasTag "INACTIVE" ;
	.

gleif-base:LegalEntity
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:FormalOrganization ,
		gleif-base:LegalPerson ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:isOrganizedIn ;
			owl:onClass lcc-cr:GeographicRegion ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "legal entity" ;
	dct:source
		"Barron's Dictionary of Banking Terms, Sixth Edition, 2012, definition of legal entity" ,
		"ISO 17442, Financial services - Legal Entity Identifier (LEI), first edition, 2012-06-01, section 3.1"
		;
	owl:disjointWith gleif-base:NaturalPerson ;
	skos:definition "a partnership, corporation, or other organization having the capacity to negotiate contracts, assume financial obligations, and pay off debts, organized under the laws of some jurisdiction" ;
	skos:example """Examples of eligible legal entities include, without limitation:
-   all financial intermediaries;
-   banks and finance companies;
-   all entities that issue equity, debt or other securities for other capital structures;
-   all entities listed on an exchange;
-   all entities that trade stock or debt, investment vehicles, including mutual funds, pension funds and alternative investment vehicles constituted as corporate entities or collective investment agreements (including umbrella funds as well as funds under an umbrella structure, hedge funds, private equity funds, etc.);
-   all entities under the purview of a financial regulator and their affiliates, subsidiaries and holding companies;
-   counterparties to financial transactions.""" ;
	skos:scopeNote "The term 'legal entities' includes, but is not limited to, unique parties that are legally or financially responsible for the performance of financial transactions or have the legal right in their jurisdiction to enter independently into legal contracts, regardless of whether they are incorporated or constituted in some other way (e.g. trust, partnership, contractual). It excludes natural persons, but includes governmental organizations and supranationals." ;
	.

gleif-base:LegalEntityRelationship
	a owl:Class ;
	rdfs:label "legal entity relationship" ;
	skos:definition "abstract superclass to represent a reified directed relationship" ;
	rdfs:subClassOf		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasSubsidiary ;
			owl:onClass gleif-base:LegalEntity ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
    [
			a owl:Restriction ;
			owl:onProperty gleif-base:hasOwner ;
			owl:onClass gleif-base:LegalEntity ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]

	.

  
gleif-base:LegalPerson
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty gleif-base:isRecognizedIn ;
		owl:someValuesFrom lcc-cr:GeographicRegion ;
	] ;
	rdfs:label "legal person"@en ;
	dct:source "http://en.wikipedia.org/wiki/Legal_personality"^^xsd:anyURI ;
	skos:definition "any entity which can incur legal obligation and can be sued at law" ;
	skos:note "To have legal personality means to be capable of having legal rights and duties within a certain legal system, such as to enter into contracts, sue, and be sued. Legal personality is a prerequisite to legal capacity, the ability of any legal person to amend (enter into, transfer, etc.) rights and obligations." ;
	.

gleif-base:NaturalPerson
	a owl:Class ;
	rdfs:subClassOf gleif-base:LegalPerson ;
	rdfs:label "natural person"@en ;
	skos:definition "A person of maturity who in the eyes of the law is able to assume obligations." ;
	.

gleif-base:Organization
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasMember ;
			owl:allValuesFrom gleif-base:LegalPerson ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasPart ;
			owl:allValuesFrom gleif-base:Organization ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:hasAddress ;
			owl:onClass gleif-base:PhysicalAddress ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "organization"@en ;
	dct:source "https://www.iso.org/obp/ui/#iso:std:iso-iec:6523:-1:ed-1:v1:en" ;
	skos:definition "a unique framework of authority within which a person or persons act, or are designated to act, towards some purpose, such as to meet a need or pursue collective goals on a continuing basis" ;
	skos:example "This may be a business entity or a government, international or not-for-profit, or other unincorporated social organization." ;
	.

gleif-base:Period
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty gleif-base:hasStart ;
		owl:qualifiedCardinality "1" ;
    owl:onDataRange xsd:dateTime ;
	] ;
	rdfs:label "period"@en ;
	skos:definition "a period of time, with at least a start time, and possibly an end." ;
	.

gleif-base:hasStart
  a owl:DatatypeProperty ;
	rdfs:label "has start"@en ;
	skos:definition "the start time of a period." ;
  rdfs:domain gleif-base:Period;
  rdfs:range xsd:dateTime;
  .

gleif-base:hasEnd
  a owl:DatatypeProperty ;
	rdfs:label "has end"@en ;
	skos:definition "the end time of a period." ;
  rdfs:domain gleif-base:Period;
  rdfs:range xsd:dateTime;
  .  
  
gleif-base:PhysicalAddress
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty gleif-base:hasCity ;
		owl:cardinality "1" ;
	] ;
	rdfs:label "physical address"@en ;
	skos:definition "a physical address where communications can be addressed, papers served or representatives located for any kind of organization or person" ;
	skos:scopeNote "An address is a collection of information, presented in a mostly fixed format, used for describing the location of a building, apartment, or other structure or a plot of land, generally using political boundaries and street names as references, along with other identifiers such as house or apartment numbers.  Some addresses also contain special codes to aid routing of mail and packages, such as a ZIP code or post code. (Wikipedia)" ;
	.

gleif-base:RegistrationAuthority
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:LegalEntity ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:manages ;
			owl:someValuesFrom gleif-base:Registry ;
		]
		;
	rdfs:label "registration authority" ;
	dct:source "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
	skos:altLabel "RA" ;
	skos:definition "a legal entity that is responsible for maintaining a registry and provides registration services" ;
	.

gleif-base:RegistrationStatus
	a owl:Class ;
	rdfs:label "registration status" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	skos:definition "a lifecycle stage indicating the status of a given registration of something, such as a business or legal entity, as specified by the registration authority" ;
	skos:note "There may be other possible values for registration status, depending on the registry, thus the individuals provided herein are not intended to be exhaustive." ;
	.

gleif-base:Registry
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:isManagedBy ;
			owl:onClass gleif-base:RegistrationAuthority ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:comprises ;
			owl:someValuesFrom gleif-base:RegistryEntry ;
		]
		;
	rdfs:label "registry" ;
	dct:source "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
	skos:definition "a system, typically an information system, that records the registration of items" ;
	.

gleif-base:RegistryEntry
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:isIncludedIn ;
			owl:allValuesFrom gleif-base:Registry ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:comprises ;
			owl:someValuesFrom gleif-base:RegistryIdentifier ;
		]
		;
	rdfs:label "registry entry" ;
	dct:source "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
	skos:definition "an entry in a registry that specifies something about the thing that is registered" ;
	.

gleif-base:RegistryIdentifier
	a owl:Class ;
	rdfs:subClassOf
		gleif-base:Identifier ,
		[
			a owl:Restriction ;
			owl:onProperty gleif-base:isRegisteredIn ;
			owl:someValuesFrom gleif-base:Registry ;
		]
		;
	rdfs:label "registry identifier" ;
	dct:source "ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15" ;
	skos:definition "an identifier associated with an entry in a registry, i.e., one that provides an index to the registry for the identified item" ;
	.

gleif-base:comprises
	a owl:ObjectProperty ;
	rdfs:label "comprises" ;
	dct:source "https://www.merriam-webster.com/dictionary/comprise"^^xsd:anyURI ;
	skos:definition "includes, especially within a particular scope, is made up of" ;
	skos:note "Note that something can be comprised of something(s) that may or may not be understood as separable parts, and thus is not defined as being explicitly transitive." ;
	.

gleif-base:hasSuccessor
	a owl:ObjectProperty ;
	rdfs:label "has successor" ;
  rdfs:domain gleif-base:LegalEntity ;
  rdfs:range gleif-base:LegalEntity ;
	skos:definition "The surviving/new legal entity which continues/replaces this registration." ;
	. 

gleif-base:records
	a owl:ObjectProperty ;
	rdfs:label "records" ;
  rdfs:domain gleif-base:RegistryEntry ;
	skos:definition "the thing which the registry entry is a record of" ;
	. 
  
gleif-base:hasASCIIName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasTransliteratedName ;
	rdfs:label "has legal name" ;
	rdfs:range xsd:string ;
	skos:definition "the name used to refer to a person or organization in 7 bit ASCII text (ISO 646); also valid UNICODE (ISO 10646)" ;
	.

gleif-base:hasAbbreviation
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf skos:altLabel ;
	rdfs:label "has abbreviation" ;
	skos:definition "an abbreviation for the entity identified" ;
	.

gleif-base:hasAdditionalName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf
		skos:altLabel ,
		gleif-base:hasName
		;
	rdfs:label "has legal name" ;
	rdfs:range xsd:string ;
	skos:definition "the name used to refer to an person or organization informally" ;
	.

gleif-base:hasAddress
	a owl:ObjectProperty ;
	rdfs:label "has address" ;
	rdfs:range gleif-base:PhysicalAddress ;
	skos:definition "has a means by which the entity may be located or contacted or may receive correspondence" ;
	.

gleif-base:hasAddressLine1
	a owl:DatatypeProperty ;
	rdfs:label "has address line 1" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "the first line of the street address" ;
	.

gleif-base:hasAddressLine2
	a owl:DatatypeProperty ;
	rdfs:label "has address line 2" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "the second line of the street address" ;
	.

gleif-base:hasAddressLine3
	a owl:DatatypeProperty ;
	rdfs:label "has address line 3" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "the third line of the street address" ;
	skos:note "This element SHALL be omitted if address line 2 is omitted." ;
	.

gleif-base:hasAddressLine4
	a owl:DatatypeProperty ;
	rdfs:label "has address line 4" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "the fourth line of the street address" ;
	skos:note "This element SHALL be omitted if address line 3 is omitted." ;
	.

gleif-base:hasAddressNumber
	a owl:DatatypeProperty ;
	rdfs:label "has address number" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "optional, additional structured version of an external house number, or range of numbers, contained in one of the address line elements" ;
	.

gleif-base:hasAddressNumberWithinBuilding
	a owl:DatatypeProperty ;
	rdfs:label "has address number within building" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "optional, additional structured version of an internal location number, or range of numbers, contained in one of the address line elements" ;
	.

gleif-base:hasCity
	a owl:DatatypeProperty ;
	rdfs:label "has city" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:range xsd:string ;
	skos:definition "the name of the city" ;
	skos:note "This property should be used in cases where a formal individual for the business center or municipality is not available.  Note that Geonames could be used as a source, however, in cases where an individual is desired." ;
	.

gleif-base:hasCountry
	a owl:ObjectProperty ;
	rdfs:label "has country" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:range lcc-cr:Country ;
	skos:definition "identifies the country according to ISO 3166-1" ;
	.

gleif-base:hasCoverageArea
	a owl:ObjectProperty ;
	rdfs:label "has coverage area" ;
	rdfs:range lcc-cr:GeographicRegion ;
	skos:definition "indicates a geographic region in which some service is provided, or to which some policy applies, or in which something is available" ;
	.

gleif-base:hasEntityExpirationDate
	a owl:DatatypeProperty ;
	rdfs:label "has entity expiration date" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:LegalEntity ;
	rdfs:range xsd:dateTime ;
	skos:definition "the date that the legal entity ceased to operate, whether due to dissolution, merger or acquisition" ;
	.

gleif-base:hasEntityExpirationReason
	a owl:ObjectProperty ;
	rdfs:label "has entity expiration reason" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:LegalEntity ;
	rdfs:range gleif-base:ExpirationReason ;
	skos:definition "the reason that a legal entity ceased to exist and/or operate" ;
	.

gleif-base:hasEntityStatus
	a owl:ObjectProperty ;
	rdfs:label "has entity status" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:LegalEntity ;
	rdfs:range gleif-base:EntityStatus ;
	skos:definition "indicates the status of the entity (i.e., active, inactive)" ;
	.

gleif-base:hasInitialRegistrationDate
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasRegistrationDate ;
	rdfs:label "has initial registration date" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:range xsd:dateTime ;
	skos:definition "the date on which an identifier or other registered item was first registered" ;
	.

gleif-base:hasLegalName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasName ;
	rdfs:label "has legal name" ;
	skos:definition "the name used to refer to an person or organization in legal communications" ;
	.

gleif-base:hasLocalAbbreviation
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasAbbreviation ;
	rdfs:label "has name" ;
	skos:definition "an abbreviation using a language local to the entity identified, may be English" ;
	.

gleif-base:hasLocalLegalName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf
		gleif-base:hasLegalName ,
		gleif-base:hasLocalName
		;
	rdfs:label "has legal name" ;
	skos:definition "the name used to refer to an person or organization in legal communications in local alphabet, which may be English" ;
	.

gleif-base:hasLocalName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasName ;
	rdfs:label "has name" ;
	skos:definition "a name in a language local to the entity identified, may be English" ;
	.

gleif-base:hasMember
	a owl:ObjectProperty ;
	rdfs:label "has member" ;
	owl:inverseOf gleif-base:isMemberOf ;
	skos:definition "relates something, typically a group or organization, to some discrete thing identified as a part (member) of it" ;
	skos:note "This property should be read as being the property of a logical union of group and organization (not shown)." ;
	.

gleif-base:hasName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf rdfs:label ;
	rdfs:label "has name" ;
	skos:definition "associates a name, reference name, or appellation with an individual concept" ;
	.

gleif-base:hasPart
	a owl:ObjectProperty ;
	rdfs:label "has part" ;
	dct:source "Stanford Encyclopedia of Philosophy at http://plato.stanford.edu/entries/mereology/" ;
	skos:definition "indicates any portion of a thing, regardless of whether the portion itself is attached to the remainder or detached; cognitively salient or arbitrarily demarcated; self-connected or disconnected; homogeneous or gerrymandered; material or immaterial; extended or unextended; spatial or temporal" ;
	skos:note "This property relates a thing to anything which is a proper part of that thing. This is not parthood in the sense of a the role of part which may be played by interchangeable things such as wheels; instead this property relates an independent thing to something which makes up a part of it." ;
	.

gleif-base:hasPostalCode
	a owl:DatatypeProperty ;
	rdfs:label "has postal code" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "the postal code of this address as specified by the local postal service" ;
	.

gleif-base:hasRegistrationDate
	a owl:DatatypeProperty ;
	rdfs:label "has registration date" ;
	rdfs:range [
		a rdfs:Datatype ;
		owl:unionOf (
			xsd:string
			xsd:dateTime
			xsd:dateTimeStamp
		) ;
	] ;
	skos:definition "the date or date and time on which something is recorded" ;
	.

gleif-base:hasLastModificationDate
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasRegistrationDate ;
	rdfs:label "has last modification date" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:range xsd:dateTime ;
	skos:definition "the date that the detail of a specific registration in the registry was last revised" ;
	.

gleif-base:hasRegistrationStatus
	a owl:ObjectProperty ;
	rdfs:label "has registration status" ;
	rdfs:range gleif-base:RegistrationStatus ;
	skos:definition "indicates the status of a specific registration, such as for an identifier or license" ;
	.

gleif-base:hasScope
	a owl:ObjectProperty ;
	rdfs:label "has scope" ;
	owl:inverseOf gleif-base:scopes ;
	skos:definition "specifies a form of words that conveys the meaning associated with something" ;
	.

gleif-base:hasNextRenewalDate
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasRegistrationDate ;
	rdfs:label "has next renewal date" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:range xsd:dateTime ;
	skos:definition "the date by which a specific registration in the registry must be renewed or updated" ;
	.

gleif-base:hasSubdivision
	a owl:ObjectProperty ;
	rdfs:label "has subdivision" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:range lcc-cr:CountrySubdivision ;
	skos:definition "identifies the country subdivision (state, province, region, etc.) according to ISO 3166-2" ;
	.
  
gleif-base:hasOwner
	a owl:ObjectProperty ;
	rdfs:label "has owner" ;
	rdfs:domain gleif-base:LegalEntityRelationship ;
	rdfs:range gleif-base:LegalEntity ;
	skos:definition "the legal entity that plays the owner role" ;
	.
  
gleif-base:hasSubsidiary
	a owl:ObjectProperty ;
	rdfs:label "has subsidiary" ;
	rdfs:domain gleif-base:LegalEntityRelationship ;
	rdfs:range gleif-base:LegalEntity ;
	skos:definition "the legal entity that plays the subsidiary role" ;
	.


gleif-base:hasTag
	a
		owl:DatatypeProperty ,
		owl:FunctionalProperty
		;
	rdfs:label "has tag" ;
	rdfs:range xsd:string ;
	skos:definition "has a unique combination of alphanumeric characters or binary representation corresponding to the identifier, code, or other element to which it applies" ;
	.

gleif-base:hasTranslatedEnglishName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasTransliteratedName ;
	rdfs:label "has legal name" ;
	rdfs:range xsd:string ;
	skos:definition "the name used to refer to a person or organization, translated into English. Transliterated by definition" ;
	.

gleif-base:hasTransliteratedAbbreviation
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasAbbreviation ;
	rdfs:label "has name" ;
	skos:definition "an abbreviation transliterated into a Western alphabet" ;
	.

gleif-base:hasTransliteratedName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gleif-base:hasAdditionalName ;
	rdfs:label "has legal name" ;
	rdfs:range xsd:string ;
	skos:definition "the name used to refer to a person or organization, converted from original to western text. Not generally the legal name" ;
	.

gleif-base:hasWebsite
	a owl:DatatypeProperty ;
	rdfs:label "has website" ;
	dct:source "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	rdfs:range xsd:anyURI ;
	skos:definition "a website associated with something, including but not limited to an organization" ;
	.

gleif-base:identifies
	a owl:ObjectProperty ;
	rdfs:label "identifies" ;
	skos:definition "is the relationship between something and that which provides a unique reference for it" ;
	.

gleif-base:isIncludedIn
	a owl:ObjectProperty ;
	rdfs:label "is included in" ;
	owl:inverseOf gleif-base:comprises ;
	skos:definition "relates something to something that contains it" ;
	.

gleif-base:isManagedBy
	a owl:ObjectProperty ;
	rdfs:label "is managed by" ;
	skos:definition "relates something to another thing that has some role in directing its affairs" ;
	.

gleif-base:isMemberOf
	a owl:ObjectProperty ;
	rdfs:label "is member of" ;
	skos:definition "belonging, either individually or collectively, to a group" ;
	skos:note "This property should be read as being framed in terms of a logical union of group and organization (not shown)." ;
	.

gleif-base:isOrganizedIn
	a owl:ObjectProperty ;
	rdfs:subPropertyOf gleif-base:isRecognizedIn ;
	rdfs:label "is organized in" ;
	rdfs:domain gleif-base:LegalEntity ;
	rdfs:range lcc-cr:GeographicRegion ;
	skos:definition "indicates the jurisdiction whose laws a legal entity is organized under" ;
	.

gleif-base:isRecognizedIn
	a owl:ObjectProperty ;
	rdfs:label "is recognized in" ;
	rdfs:domain gleif-base:LegalEntity ;
	rdfs:range lcc-cr:GeographicRegion ;
	skos:definition "indicates the jurisdiction in which a legal person is authorized to conduct business or an agreement may be acknowledged and possibly enforceable" ;
	.

gleif-base:isRegisteredIn
	a owl:ObjectProperty ;
	rdfs:label "is registered in" ;
	rdfs:range gleif-base:Registry ;
	skos:definition "indicates the registry that something is registered in" ;
	.

gleif-base:mailRouting
	a owl:DatatypeProperty ;
	rdfs:label "mail routing" ;
	dct:source "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	rdfs:domain gleif-base:PhysicalAddress ;
	rdfs:range xsd:string ;
	skos:definition "optional free text address line to hold content from other address lines containing explicit routing information (this element's presence indicates that this address is a routing / \"care of\" address." ;
	.

gleif-base:manages
	a owl:ObjectProperty ;
	rdfs:label "manages" ;
	owl:inverseOf gleif-base:isManagedBy ;
	skos:definition "relates an autonomous agent to something that it directs in some way" ;
	.

gleif-base:scopes
	a owl:ObjectProperty ;
	rdfs:label "scopes" ;
	skos:definition "determines or identifies the essential qualities or meaning of, discovers and sets forth the meaning of, fixes or marks the limits of, demarcates" ;
	.

