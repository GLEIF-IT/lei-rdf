<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY gleif-base "https://www.gleif.org/ontology/Base/">
	<!ENTITY gleif-elf "https://www.gleif.org/ontology/ELF/">
	<!ENTITY lcc-cr "http://www.omg.org/spec/LCC/Countries/CountryRepresentation/">
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://www.gleif.org/ontology/ELF/"
	xmlns:gleif-base="https://www.gleif.org/ontology/Base/"
  xmlns:gleif-elf="https://www.gleif.org/ontology/ELF/"
	xmlns:lcc-cr="http://www.omg.org/spec/LCC/Countries/CountryRepresentation/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://www.gleif.org/ontology/ELF/">
		<rdfs:label>GLEIF Entity Legal Form Ontology</rdfs:label>
		<dct:abstract>This ontology defines concepts for Entity Legal Forms</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dct:license>
		<dct:rights>Copyright (c) 2018 GLEIF</dct:rights>
    <owl:imports rdf:resource="https://www.gleif.org/ontology/Base/"/>
    <owl:imports rdf:resource="http://www.omg.org/spec/LCC/Countries/ISO3166-1-CountryCodes-Add/"/>
    <owl:imports rdf:resource="http://www.omg.org/spec/LCC/Countries/ISO3166-2-SubdivisionCodes-Add/"/>
	</owl:Ontology>

 
  <owl:Class rdf:about="&gleif-elf;EntityLegalForm">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;hasLocalAbbreviation"/>
        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
        <owl:onDataRange rdf:resource="&xsd;string"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;hasTransliteratedAbbreviation"/>
        <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
        <owl:onDataRange rdf:resource="&xsd;string"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;haslocalName"/>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;hasTransliteratedName"/>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="gleif-base:hasCoverageArea"/>
        <owl:someValuesFrom rdf:resource="&lcc-cr;GeographicRegion"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;hasRegistrationStatus"/>
        <owl:someValuesFrom rdf:resource="&gleif-elf;ELFRegistrationStatus"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;hasRegistrationRevisionDate"/>
        <owl:onDataRange rdf:resource="&xsd;dateTime"/>
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label>entity legal form</rdfs:label>
    <skos:definition>a classifier for a legal entity that indicates the nature of that entity as defined from a legal or regulatory perspective, in the jurisdiction in which it was established</skos:definition>
    <dct:source rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</dct:source>
  </owl:Class>
  
  <owl:Class rdf:about="&gleif-elf;EntityLegalFormIdentifier">
    <rdfs:subClassOf rdf:resource="&gleif-base;Identifier"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;identifies"/>
        <owl:onClass rdf:resource="&gleif-elf;EntityLegalForm"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&gleif-base;isRegisteredIn"/>
        <owl:hasValue rdf:resource="&gleif-elf;ELFregistry"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>entity legal form identifier</rdfs:label>
    <skos:definition>a code that uniquely identifies an entity legal form as defined in ISO 20275</skos:definition>
    <dct:source rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</dct:source>
  </owl:Class>
  <owl:Class rdf:about="&gleif-elf;ELFRegistrationStatus">
    <rdfs:subClassOf rdf:resource="&gleif-base;RegistrationStatus"/>
    <rdfs:label>ELF registration status</rdfs:label>
    <skos:definition>status of an entry in a entity legal form registry that conforms to ISO 20275 and the Global Legal Entity Identifier Foundation (GLEIF) Common Data Format (CDF)</skos:definition>
  </owl:Class>
  
  <owl:NamedIndividual rdf:about="&gleif-elf;ActiveStatus">
    <rdf:type rdf:resource="&gleif-elf;ELFRegistrationStatus"/>
    <rdfs:label>active status</rdfs:label>
    <skos:definition>status indicating that the registration is active</skos:definition>
    <gleif-base:hasTag>ACTV</gleif-base:hasTag>
    <dct:source rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</dct:source>
  </owl:NamedIndividual>
  
  <owl:NamedIndividual rdf:about="&gleif-elf;InactiveStatus">
    <rdf:type rdf:resource="&gleif-elf;ELFRegistrationStatus"/>
    <rdfs:label>inactive status</rdfs:label>
    <skos:definition>status indicating that the registration is inactive</skos:definition>
    <gleif-base:hasTag>INAC</gleif-base:hasTag>
    <dct:source rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1</dct:source>
  </owl:NamedIndividual>
  
  
  <gleif-base:Registry rdf:about="&gleif-elf;ELFregistry">
    <rdfs:label>GLEIF Entity Legal Forms List</rdfs:label>
    <skos:altLabel>ELF</skos:altLabel>
    <gleif-base:hasWebsite rdf:datatype="&xsd;anyURI">https://www.gleif.org/en/about-lei/code-lists/iso-20275-entity-legal-forms-code-list</gleif-base:hasWebsite>
    <gleif-base:isManagedBy rdf:resource="&gleif-base;GLEIF"/>
  </gleif-base:Registry>
  
  
</rdf:RDF>